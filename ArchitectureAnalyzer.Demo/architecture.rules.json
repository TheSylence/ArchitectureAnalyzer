{
  "rules": [
    {
      "mustInherit": {
        "forTypes": { "name": "*Attribute" },
        "baseType": { "fullName": "System.Attribute" }
      }
    },
    {
      "mustImplement": {
        "forTypes": { "name": "*Disposable" },
        "interface": { "fullName": "System.IDisposable" }
      }
    }
  ]
}